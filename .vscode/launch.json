  "version": "0.2.0",
  "configurations": [
    {
      "type": "pwa-node",
      "request": "attach",
      "name": "Attach to Node",
      "skipFiles": [
        "<node_internals>/**"
      ],
      "localRoot": "${workspaceFolder}/src",
      "remoteRoot": "/app/src",
      "port": 9229,
      "outFiles": [
        "${workspaceFolder}/src/**/*.js"
      ]
    },
  ]
}